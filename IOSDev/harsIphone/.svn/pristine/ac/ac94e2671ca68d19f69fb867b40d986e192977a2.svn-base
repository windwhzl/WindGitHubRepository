<script>
function voil(obj){
	var info=document.getElementById("info");
	if(obj.name.value&&obj.tel.value&&obj.bname.value&&obj.baddress.value&&obj.title.value&&obj.content.value&&obj.code.value.length==4){
		return true;
	}else if(!obj.name.value){
		info.innerHTML="请填写 举报投诉人姓名";
	}else if(!obj.tel.value){
		info.innerHTML="请填写 举报投诉联系电话";
	}else if(!obj.bname.value){
		info.innerHTML="请填写 被举报投诉单位或个人名称";
	}else if(!obj.baddress.value){
		info.innerHTML="请填写 被举报投诉单位或个人地址";
	}else if(!obj.title.value){
		info.innerHTML="请填写 举报投诉主题";
	}else if(!obj.content.value){
		info.innerHTML="请填写 举报投诉详细内容";
	}else if(!obj.code.value || obj.code.value.length!=4){
		info.innerHTML="请填写 附加码";
	}
	return false;
}
</script>
</head>
<body>
<script type="text/javascript" src="/web/harsj/js/header2.js"></script>
