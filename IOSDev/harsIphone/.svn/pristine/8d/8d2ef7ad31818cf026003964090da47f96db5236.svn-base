- (IBAction)moreFunction:(id)sender {
    AlertDialogWhenFunctionNotComplete *aa=[[AlertDialogWhenFunctionNotComplete alloc] initWithFrame:CGRectMake(60.0, 120.0, 200.0, 200.0)];
    [aa show];
    [self.view addSubview:aa];
}
